#, c-format
msgid "%u byte"
msgid_plural "%u bytes"
msgstr[0] "%u bájt"
msgstr[1] "%u bájt"

#: ../glib/gfileutils.c:1785
#, c-format
msgid "%.1f KB"
msgstr "%.1f KB"

#: ../glib/gfileutils.c:1790
#, c-format
msgid "%.1f MB"
msgstr "%.1f MB"

#: ../glib/gfileutils.c:1795
#, c-format
msgid "%.1f GB"
msgstr "%.1f GB"

#: ../glib/gfileutils.c:1800
#, c-format
msgid "%.1f TB"
msgstr "%.1f TB"

#: ../glib/gfileutils.c:1805
#, c-format
msgid "%.1f PB"
msgstr "%.1f PB"

#: ../glib/gfileutils.c:1810
#, c-format
msgid "%.1f EB"
msgstr "%.1f EB"

#: ../glib/gfileutils.c:1853
#, c-format
